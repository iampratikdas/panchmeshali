{
  "name": "panchmeshali-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "_dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "prettier-format": "prettier --config .prettierrc.config.js app/**/*.jsx --write",
    "prettier-watch": "onchange \"app/**/*\" -- prettier --config .prettierrc.config.js --write --ignore-unknown {{changed}}",
    "dev": "concurrently \"npm:_dev\" \"npm:prettier-watch\""
  },
  "dependencies": {
    "next": "14.1.0",
    "react": "^18",
    "react-dom": "^18"
  },
  "devDependencies": {
    "autoprefixer": "^10.0.1",
    "concurrently": "^8.2.2",
    "eslint": "^8",
    "eslint-config-next": "14.1.0",
    "onchange": "^7.1.0",
    "postcss": "^8",
    "prettier": "3.2.5",
    "tailwindcss": "^3.3.0"
  }
}
